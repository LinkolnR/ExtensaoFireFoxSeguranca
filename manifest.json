{
    "manifest_version": 2,
    "name": "FirefoxTechackerProject",
    "version": "0.1",
    "description": "Uma extensão que captura conexões com domínios de terceiros e exibe em um popup.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "activeTab",
      "tabs",
      "storage",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png",
      "96": "icon.png"
    }
  }
  